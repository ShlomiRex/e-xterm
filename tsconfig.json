{
	"compilerOptions": {
		"allowJs": true,
		"module": "CommonJS",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"baseUrl": ".",
		"outDir": "dist",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"paths": {
			"*": [
				"node_modules/*"
			]
		},
		"jsx": "react"
	},
	"include": [
		"src/**/*"
	]
}