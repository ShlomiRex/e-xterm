{
	"name": "e-xterm",
	"version": "1.0.0",
	"description": "e-xterm is cross-platform electron SSH and SFTP program, which aims to be used for commercial/professional users.",
	"main": "src/main/main.ts",
	"license": "MIT",
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"watch2": "nodemon -e ts -w ./src -x npm run watch:serve",
		"watch:serve": "ts-node --inspect src/main/main.ts",
		"lint": "eslint -c .eslintrc --ext .ts ./src",
		"start": "npm run install-local-packages && npm run build && electron ./dist/main/main.js",
		"install-local-packages": "npm install C:\\Users\\Shlomi\\Desktop\\electron-browser"
	},
	"repository": "https://github.com/ShlomiRex/e-xterm",
	"author": "Shlomi Domnenko",
	"dependencies": {
		"bootstrap": "^5.0.0-beta1",
		"electron": "^11.2.0",
		"electron-browser": "file:../electron-browser",
		"electron-store": "^7.0.0",
		"jquery": "^3.5.1",
		"node-cmd": "^4.0.0",
		"node-pty": "^0.9.0",
		"socket.io": "^3.1.0",
		"split.js": "^1.6.2",
		"ssh2": "^0.8.9",
		"typescript": "^4.1.3",
		"uuid": "^3.4.0",
		"w2ui": "^1.4.3",
		"xterm": "^4.9.0",
		"xterm-addon-attach": "^0.6.0",
		"xterm-addon-fit": "^0.4.0"
	},
	"devDependencies": {
		"@types/ssh2": "^0.5.46",
		"@types/uuid": "^8.3.0",
		"@types/w2ui": "^1.4.32",
		"@types/websocket": "^1.0.1",
		"electron-rebuild": "^2.3.4",
		"electron-reload": "^1.5.0",
		"electron-reloader": "^1.2.0"
	}
}
